<xsd:schema xmlns:netex="http://www.netex.org.uk/netex" xmlns="http://www.netex.org.uk/netex" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:gml="http://www.opengis.net/gml/3.2" targetNamespace="http://www.netex.org.uk/netex" elementFormDefault="qualified" attributeFormDefault="qualified" id="NewDataSet">
	<xsd:import namespace="http://www.opengis.net/gml/3.2" schemaLocation="gml-pos.xsd"/>
	<xsd:complexType name="name">
		<xsd:sequence>
			<xsd:element name="Name" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="version" type="xsd:string" use="required" form="unqualified"/>
		<xsd:attribute name="id" type="xsd:string" use="required" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="ref">
		<xsd:attribute name="ref" type="xsd:string" use="required" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="refVersion">
		<xsd:attribute name="version" type="xsd:string" use="required" form="unqualified"/>
		<xsd:attribute name="ref" type="xsd:string" use="required" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="refClass">
		<xsd:attribute name="nameOfRefClass" type="xsd:string" use="required" form="unqualified"/>
		<xsd:attribute name="ref" type="xsd:string" use="required" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="location">
		<xsd:sequence>
			<xsd:element ref="gml:pos"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="modification">
		<xsd:restriction base="xsd:NMTOKEN">
			<xsd:enumeration value="new">
				<xsd:annotation>
					<xsd:documentation>This is a definition of a new entity.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="delete">
				<xsd:annotation>
					<xsd:documentation>This is a deletion of an existing entity.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="revise">
				<xsd:annotation>
					<xsd:documentation>This is a revision to an existing entity. All values are replaced.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="delta"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Colour">
		<xsd:restriction base="xsd:hexBinary">
			<xsd:maxLength value="6"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="projections">
		<xsd:sequence>
			<xsd:element name="PointProjectionRef" type="refClass" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="pointOnRoute">
		<xsd:sequence>
			<xsd:choice>
				<xsd:element name="ActivationPointRef" type="ref" minOccurs="0"/>
				<xsd:element name="RoutePointRef" type="ref" minOccurs="0"/>
			</xsd:choice>
		</xsd:sequence>
		<xsd:attribute name="version" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="id" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="order" type="xsd:nonNegativeInteger" form="unqualified"/>
		<xsd:attribute name="modification" type="modification" use="optional" default="new" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="route">
		<xsd:sequence>
			<xsd:element name="Name" type="xsd:string" minOccurs="0"/>
			<xsd:element name="LineRef" type="refVersion" minOccurs="0"/>
			<xsd:element name="pointsInSequence" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="PointOnRoute" type="pointOnRoute" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="version" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="id" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="modification" type="modification" use="optional" default="new" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="routePoint">
		<xsd:sequence>
			<xsd:element name="Location" type="location"/>
		</xsd:sequence>
		<xsd:attribute name="version" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="id" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="modification" type="modification" use="optional" default="new" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="keyList">
		<xsd:sequence>
			<xsd:element name="KeyValue" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Key" type="xsd:string"/>
						<xsd:element name="Value" type="xsd:string"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="activationPoint">
		<xsd:sequence>
			<xsd:element name="keyList" type="keyList" minOccurs="0"/>
			<xsd:element name="Location" type="location" minOccurs="0"/>
			<xsd:element name="PrivateCode" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="version" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="id" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="modification" type="modification" use="optional" default="new" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="scheduledStopPoint">
		<xsd:sequence>
			<xsd:element name="keyList" type="keyList"/>
			<xsd:element name="Name" type="xsd:string"/>
			<xsd:element name="projections" type="projections"/>
		</xsd:sequence>
		<xsd:attribute name="version" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="id" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="modification" type="modification" use="optional" default="new" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="dataSource">
		<xsd:sequence>
			<xsd:element name="Name" type="xsd:string" minOccurs="0"/>
			<xsd:element name="ShortName" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Description" type="xsd:string"/>
			<xsd:element name="Email" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="version" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="id" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="modification" type="modification" use="optional" default="new" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="availabilityCondition">
		<xsd:sequence>
			<xsd:element name="Name" type="xsd:string" minOccurs="0"/>
			<xsd:element name="FromDate" type="xsd:dateTime"/>
			<xsd:element name="ToDate" type="xsd:dateTime"/>
			<xsd:element name="ValidDayBits">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:pattern value="[0-1]+"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="version" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="id" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="modification" type="modification" use="optional" default="new" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="serviceJourney">
		<xsd:sequence>
			<xsd:element name="validityConditions">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="AvailabilityConditionRef" type="ref" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="keyList" type="keyList" minOccurs="0"/>
			<xsd:element name="TypeOfProductCategoryRef" type="ref"/>
			<xsd:element name="Monitored" type="xsd:boolean" minOccurs="0"/>
			<xsd:element name="DepartureTime" type="xsd:string"/>
			<xsd:element name="DepartureDayOffset" type="xsd:integer" default="0"/>
			<xsd:element name="JourneyPatternRef" type="refClass"/>
			<xsd:element name="TimeDemandTypeRef" type="refVersion"/>
			<xsd:element name="BlockRef" type="refVersion" minOccurs="0"/>
			<xsd:element name="OperatorRef" type="ref" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="version" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="id" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="dataSourceRef" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="modification" type="modification" use="optional" default="new" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="responsibilitySet">
		<xsd:sequence>
			<xsd:element name="Name" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="version" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="id" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="modification" type="modification" use="optional" default="new" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="noticeAssignment">
		<xsd:sequence>
			<xsd:element name="NoticeRef" type="ref"/>
			<xsd:element name="NoticedObjectRef" type="ref"/>
		</xsd:sequence>
		<xsd:attribute name="version" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="id" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="modification" type="modification" use="optional" default="new" form="unqualified"/>
		<xsd:attribute name="order" type="xsd:nonNegativeInteger" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="frameDefaults">
		<xsd:sequence>
			<xsd:element name="DefaultDataSourceRef" type="ref" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="version">
		<xsd:sequence>
			<xsd:element name="StartDate" type="xsd:dateTime"/>
			<xsd:element name="EndDate" type="xsd:dateTime"/>
			<xsd:element name="Description" type="xsd:string" minOccurs="0"/>
			<xsd:element name="VersionType" minOccurs="0">
				<xsd:simpleType>
					<xsd:annotation>
						<xsd:documentation>Allowed values for Types of VERSION.</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="point"/>
						<xsd:enumeration value="baseline"/>
						<xsd:enumeration value="delta"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="DerivedFromVersionRef" type="refVersion" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="id" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="version" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="modification" type="modification" use="optional" default="new" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="versions">
		<xsd:sequence>
			<xsd:element name="Version" type="version" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="line">
		<xsd:sequence>
			<xsd:element name="keyList" type="keyList" minOccurs="0"/>
			<xsd:element name="Name" type="xsd:string"/>
			<xsd:element name="TransportMode" type="xsd:string"/>
			<xsd:element name="PublicCode" type="xsd:string"/>
			<xsd:element name="AuthorityRef" type="ref" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="Monitored" type="xsd:boolean"/>
			<xsd:element name="Presentation" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Colour" type="Colour" minOccurs="0"/>
						<xsd:element name="TextColour" type="Colour" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="AccessibilityAssessment">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="MobilityImpairedAccess" type="xsd:boolean" minOccurs="0"/>
						<xsd:element name="Comment" minOccurs="0"/>
					</xsd:sequence>
					<xsd:attribute name="version" type="xsd:string" form="unqualified"/>
					<xsd:attribute name="id" type="xsd:string" form="unqualified"/>
					<xsd:attribute name="modification" type="modification" use="optional" default="new" form="unqualified"/>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="version" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="id" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="modification" type="modification" use="optional" default="new" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="block">
		<xsd:attribute name="version" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="id" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="modification" type="modification" use="optional" default="new" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="blocks">
		<xsd:sequence>
			<xsd:element name="Block" type="block" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="vehicleScheduleFrame">
		<xsd:sequence>
			<xsd:element name="Name" type="xsd:string" minOccurs="0"/>
			<xsd:element name="blocks" type="blocks"/>
		</xsd:sequence>
		<xsd:attribute name="version" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="id" type="xsd:string" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="timetableFrame">
		<xsd:sequence>
			<xsd:element name="Monitored" type="xsd:boolean" minOccurs="0"/>
			<xsd:element name="contentValidityConditions">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="AvailabilityCondition" type="availabilityCondition" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="OperatorView">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="OperatorRef" type="ref"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="vehicleJourneys">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="ServiceJourney" type="serviceJourney" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="version" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="id" type="xsd:string" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="stopPointInJourneyPattern">
		<xsd:sequence>
			<xsd:element name="ScheduledStopPointRef" type="ref"/>
			<xsd:element name="OnwardTimingLinkRef" type="ref" minOccurs="0"/>
			<xsd:element name="IsWaitPoint" type="xsd:boolean" minOccurs="0"/>
			<xsd:element name="ForAlighting" type="xsd:boolean" minOccurs="0"/>
			<xsd:element name="ForBoarding" type="xsd:boolean" minOccurs="0"/>
			<xsd:element name="DestinationDisplayRef" type="ref" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="version" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="id" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="modification" type="modification" use="optional" default="new" form="unqualified"/>
		<xsd:attribute name="order" type="xsd:nonNegativeInteger" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="timingPointInJourneyPattern">
		<xsd:sequence>
			<xsd:element name="TimingPointRef" type="ref"/>
			<xsd:element name="OnwardTimingLinkRef" type="ref" minOccurs="0"/>
			<xsd:element name="IsWaitPoint" type="xsd:boolean" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="version" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="id" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="modification" type="modification" use="optional" default="new" form="unqualified"/>
		<xsd:attribute name="order" type="xsd:nonNegativeInteger" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="serviceJourneyPattern">
		<xsd:sequence>
			<xsd:element name="Name" type="xsd:string" minOccurs="0"/>
			<xsd:element name="RouteRef" type="ref"/>
			<xsd:element name="DestinationDisplayRef" type="ref" minOccurs="0"/>
			<xsd:element name="pointsInSequence">
				<xsd:complexType>
					<xsd:choice minOccurs="2" maxOccurs="unbounded">
						<xsd:element name="StopPointInJourneyPattern" type="stopPointInJourneyPattern" minOccurs="0"/>
						<xsd:element name="TimingPointInJourneyPattern" type="timingPointInJourneyPattern" minOccurs="0"/>
					</xsd:choice>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="version" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="id" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="modification" type="modification" use="optional" default="new" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="timingLink">
		<xsd:sequence>
			<xsd:element name="Distance" type="xsd:nonNegativeInteger"/>
			<xsd:element name="FromPointRef" type="ref"/>
			<xsd:element name="ToPointRef" type="ref"/>
		</xsd:sequence>
		<xsd:attribute name="version" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="id" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="modification" type="modification" use="optional" default="new" form="unqualified"/>
		<xsd:attribute name="responsibilitySetRef" type="xsd:string" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="journeyRunTime">
		<xsd:sequence>
			<xsd:element name="TimingLinkRef" type="refVersion"/>
			<xsd:element name="RunTime" type="xsd:duration"/>
		</xsd:sequence>
		<xsd:attribute name="version" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="id" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="modification" type="modification" use="optional" default="new" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="journeyWaitTime">
		<xsd:sequence>
			<xsd:choice>
				<xsd:element name="ScheduledStopPointRef" type="refVersion"/>
				<xsd:element name="TimingPointRef" type="refVersion"/>
			</xsd:choice>
			<xsd:element name="WaitTime" type="xsd:duration"/>
		</xsd:sequence>
		<xsd:attribute name="version" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="id" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="modification" type="modification" use="optional" default="new" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="timeDemandType">
		<xsd:sequence>
			<xsd:element name="Name" type="xsd:string" minOccurs="0"/>
			<xsd:element name="runTimes">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="JourneyRunTime" type="journeyRunTime" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="waitTimes" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="JourneyWaitTime" type="journeyWaitTime" minOccurs="0" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="version" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="id" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="modification" type="modification" use="optional" default="new" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="timingPoint">
		<xsd:sequence>
			<xsd:element name="keyList" type="keyList" minOccurs="0"/>
			<xsd:element name="Name" type="xsd:string"/>
			<xsd:element name="projections" type="projections"/>
		</xsd:sequence>
		<xsd:attribute name="version" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="id" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="modification" type="modification" use="optional" default="new" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="notice">
		<xsd:sequence>
			<xsd:element name="Name" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Text" type="xsd:string"/>
		</xsd:sequence>
		<xsd:attribute name="version" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="id" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="modification" type="modification" use="optional" default="new" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="destinationDisplay">
		<xsd:sequence>
			<xsd:element name="Name" type="xsd:string"/>
			<xsd:element name="ShortName" type="xsd:string" minOccurs="0"/>
			<xsd:element name="vias" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Via" minOccurs="0" maxOccurs="unbounded">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="Name" type="xsd:string"/>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="version" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="id" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="modification" type="modification" use="optional" default="new" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="resourceFrame">
		<xsd:sequence>
			<xsd:element name="dataSources">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="DataSource" type="dataSource" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="responsibilitySets">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="ResponsibilitySet" type="responsibilitySet" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="typesOfValue">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="TypeOfProductCategory" type="name" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="organisations">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Operator" type="name" maxOccurs="unbounded"/>
						<xsd:element name="Authority" type="name" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="version" type="xsd:string" form="unqualified"/>
		<xsd:attribute name="id" type="xsd:string" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="serviceFrame">
		<xsd:sequence>
			<xsd:element name="routePoints" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="RoutePoint" type="routePoint" maxOccurs="unbounded"/>
