	<!-- ABSTRACTE TYPEN                                                                                  -->
	<!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
	<xsd:complexType name="identifiedObject_" abstract="true">
		<xsd:annotation>
			<xsd:documentation>Ieder object heeft een uniek 'id' conform de afspraken in het Nederlands NeTEx Profiel document</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="id" type="xsd:string" use="required" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="versionedObject_" abstract="true">
		<xsd:annotation>
			<xsd:documentation>De waarde 'any' van attribuut 'version' wordt geinterpreteerd als 'de van het bovenliggende object geërfde versie'</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="id" type="xsd:string" use="required" form="unqualified"/>
		<xsd:attribute name="version" type="xsd:string" use="optional" default="any" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="modifiedObject_" abstract="true">
		<xsd:annotation>
			<xsd:documentation>Als attribuut 'modification' ontbreekt wordt de waarde van het bovenliggend object geërfd</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="id" type="xsd:string" use="required" form="unqualified"/>
		<xsd:attribute name="modification" type="modification" use="optional" form="unqualified"/>
		<xsd:attribute name="version" type="xsd:string" use="optional" default="any" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="orderedObject_" abstract="true">
		<xsd:annotation>
			<xsd:documentation>Bij een object in een sequence is attribuut 'order' verplicht</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="id" type="xsd:string" use="required" form="unqualified"/>
		<xsd:attribute name="modification" type="modification" use="optional" form="unqualified"/>
		<xsd:attribute name="version" type="xsd:string" use="optional" default="any" form="unqualified"/>
		<xsd:attribute name="order" type="xsd:nonNegativeInteger" use="required" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="versionedFrame_" abstract="true">
		<xsd:annotation>
			<xsd:documentation>Bij een Frame is attribuut 'version' verplicht</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="id" type="xsd:string" use="required" form="unqualified"/>
		<xsd:attribute name="version" type="xsd:string" use="required" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="modifiedFrame_" abstract="true">
		<xsd:annotation>
			<xsd:documentation>Bij het CompositeFrame zijn attributen 'version' en 'modification' verplicht</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="id" type="xsd:string" use="required" form="unqualified"/>
		<xsd:attribute name="modification" type="modification" use="required" form="unqualified"/>
		<xsd:attribute name="version" type="xsd:string" use="required" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="modifiedVersion_" abstract="true">
		<xsd:annotation>
			<xsd:documentation>Bij een Version zijn attributen 'version' en 'modification' verplicht</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="id" type="xsd:string" use="required" form="unqualified"/>
		<xsd:attribute name="modification" type="modification" use="required" form="unqualified"/>
		<xsd:attribute name="version" type="xsd:string" use="required" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="predefinedObject_" abstract="true">
		<xsd:annotation>
			<xsd:documentation>Een element uit een voorgedefinieerde lijst heeft alleen een uniek 'id', geen 'version'</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="id" type="xsd:string" use="required" form="unqualified"/>
	</xsd:complexType>
	<!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
	<!-- REFERENTIES                                                                                           -->
	<!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
	<xsd:complexType name="ref">
		<xsd:attribute name="ref" type="xsd:string" use="required" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="refVersion">
		<xsd:attribute name="version" type="xsd:string" use="optional" default="any" form="unqualified"/>
		<xsd:attribute name="ref" type="xsd:string" use="required" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="refClass">
		<xsd:attribute name="nameOfRefClass" type="xsd:string" use="required" form="unqualified"/>
		<xsd:attribute name="ref" type="xsd:string" use="required" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="refClassVersion">
		<xsd:attribute name="nameOfRefClass" type="xsd:string" use="required" form="unqualified"/>
		<xsd:attribute name="version" type="xsd:string" use="optional" default="any" form="unqualified"/>
		<xsd:attribute name="ref" type="xsd:string" use="required" form="unqualified"/>
	</xsd:complexType>
	<xsd:complexType name="refType">
		<xsd:attribute name="ref" type="xsd:string" use="required" form="unqualified"/>
		<xsd:attribute name="type" type="xsd:string" use="required" form="unqualified"/>
	</xsd:complexType>
