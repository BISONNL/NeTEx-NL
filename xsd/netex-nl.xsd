	<!-- BASIS TYPEN                                                                                              -->
	<!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
	<xsd:complexType name="location">
		<xsd:sequence>
			<xsd:element ref="gml:pos"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="typedString">
		<xsd:simpleContent>
			<xsd:extension base="xsd:string">
				<xsd:attribute name="textIdType" type="xsd:string" use="optional" form="unqualified"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="privateCode">
		<xsd:simpleContent>
			<xsd:extension base="xsd:string">
				<xsd:attribute name="type" type="xsd:string" use="required" form="unqualified"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="iconURI">
		<xsd:simpleContent>
			<xsd:extension base="xsd:anyURI">
				<xsd:attribute name="typeOfInfoLink" type="xsd:string" fixed="icon"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:simpleType name="websiteURI">
		<xsd:restriction base="xsd:anyURI">
			<xsd:maxLength value="1024"/>
			<xsd:pattern value="(http|HTTP|https|HTTPS)://.+"/>
			<!-- http/https (lowercase or uppercase, no combinations) -->
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="imageURI">
		<xsd:restriction base="xsd:anyURI">
			<xsd:maxLength value="1024"/>
			<xsd:pattern value="(http|HTTP|https|HTTPS|ftp|FTP)://.+\.(jpg|JPG|jpeg|JPEG|gif|GIF|png|PNG|svg|SVG)"/>
			<!-- ftp/http/https (lowercase or uppercase, no combinations) -->
			<!-- file types jpg, jpeg, gif, png,svg (lowercase or uppercase, no combinations) -->
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="colour">
		<xsd:restriction base="xsd:hexBinary">
			<xsd:maxLength value="6"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
	<!-- UITBREIDINGEN VAN DE NETEX STANDAARD                                       -->
	<!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
	<xsd:complexType name="extensionPrivateCode">
		<xsd:sequence>
			<xsd:element name="PrivateCode" type="privateCode"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="keyList">
		<xsd:sequence>
			<xsd:element name="KeyValue" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Key" type="xsd:string"/>
						<xsd:element name="Value" type="xsd:string"/>
					</xsd:sequence>
					<xsd:attribute name="typeOfKey" type="xsd:normalizedString"/>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
