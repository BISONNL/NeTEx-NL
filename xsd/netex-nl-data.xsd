							<xsd:list itemType="PassengerCommsFacilityEnumeration"/>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="SanitaryFacilityList" minOccurs="0">
						<xsd:simpleType>
							<xsd:list itemType="SanitaryFacilityEnumeration"/>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="TicketingServiceFacilityList" minOccurs="0">
						<xsd:simpleType>
							<xsd:list itemType="TicketingServiceFacilityEnumeration"/>
						</xsd:simpleType>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
</xsd:schema>
