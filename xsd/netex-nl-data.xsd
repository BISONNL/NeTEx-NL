						<xsd:annotation>
							<xsd:documentation>Hier altijd type="VehicleNumber" gebruiken</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="OperatorRef" type="VersionOfObjectRefStructure" minOccurs="0"/>
					<xsd:element name="VehicleTypeRef" type="VersionOfObjectRefStructure"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="vehicleType">
		<xsd:complexContent>
			<xsd:extension base="DataManagedObjectStructure">
				<xsd:sequence>
					<xsd:element name="Name" type="MultilingualString"/>
					<xsd:element name="ShortName" type="MultilingualString" minOccurs="0"/>
					<xsd:element name="Description" type="MultilingualString" minOccurs="0"/>
					<xsd:element name="PrivateCode" type="PrivateCodeStructure" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Hier altijd type="VoertuigTypeCode" gebruiken</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="TypeOfFuel" type="TypeOfFuelEnumeration" minOccurs="0"/>
					<xsd:element name="PassengerCapacity" minOccurs="0">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="SeatingCapacity" type="xsd:nonNegativeInteger" minOccurs="0"/>
								<xsd:element name="StandingCapacity" type="xsd:nonNegativeInteger" minOccurs="0"/>
								<xsd:element name="SpecialPlaceCapacity" type="xsd:nonNegativeInteger" minOccurs="0"/>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="LowFloor" type="xsd:boolean" minOccurs="0"/>
					<xsd:element name="HasLiftOrRamp" type="xsd:boolean" minOccurs="0"/>
					<xsd:element name="HasHoist" type="xsd:boolean" minOccurs="0"/>
					<xsd:element name="BoardingHeight" type="LengthType" minOccurs="0"/>
					<xsd:element name="GapToPlatform" type="LengthType" minOccurs="0"/>
					<xsd:element name="Length" type="LengthType"/>
					<xsd:element name="Width" type="LengthType" minOccurs="0"/>
					<xsd:element name="Height" type="LengthType" minOccurs="0"/>
					<xsd:element name="Weight" type="WeightType" minOccurs="0"/>
					<xsd:element name="facilities" type="facilities" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
