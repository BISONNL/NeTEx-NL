		<xsd:sequence>
			<xsd:element name="Extensions" type="extensionViaOrder" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>De volgorde van de Via's</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Name" type="MultilingualString"/>
			<!--
			<xsd:element name="ViaType" type="ViaTypeEnumeration" minOccurs="0" default="name">
				<xsd:annotation>
					<xsd:documentation>Mogelijke waarden: name, stopPoint, other</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
-->
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="propertyOfDay">
		<xsd:sequence>
			<xsd:element name="DaysOfWeek" type="DaysOfWeekListOfEnumerations" default="Everyday" minOccurs="0"/>
			<xsd:element name="WeeksOfMonth" type="WeeksOfMonthListOfEnumerations" default="EveryWeek" minOccurs="0"/>
			<xsd:element name="DayOfYear" type="xsd:gMonthDay" minOccurs="0"/>
			<xsd:element name="HolidayTypes" type="HolidayTypesListOfEnumerations" default="AnyDay" minOccurs="0"/>
			<xsd:element name="Seasons" type="SeasonsListOfEnumerations" default="Perennially" minOccurs="0"/>
			<xsd:element name="Tides" type="TidesListOfEnumerations" default="AllTides" minOccurs="0"/>
			<xsd:element name="DayEvent" type="DayEventEnumeration" minOccurs="0"/>
			<xsd:element name="Crowding" type="CrowdingEnumeration" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="dayType">
		<xsd:complexContent>
			<xsd:extension base="DataManagedObjectStructure">
				<xsd:sequence>
					<xsd:element name="Name" type="MultilingualString" minOccurs="0"/>
					<xsd:element name="ShortName" type="MultilingualString" minOccurs="0"/>
					<xsd:element name="properties">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="PropertyOfDay" type="propertyOfDay" maxOccurs="unbounded"/>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="dayTypeAssignment">
		<xsd:complexContent>
			<xsd:extension base="OrderedVersionOfObjectStructure-fixed">
				<xsd:sequence>
					<xsd:element name="Date" type="xsd:date"/>
					<xsd:element name="DayTypeRef" type="VersionOfObjectRefStructure"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="vehicle">
		<xsd:complexContent>
			<xsd:extension base="DataManagedObjectStructure">
				<xsd:sequence>
					<xsd:element name="ValidBetween">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="FromDate" type="xsd:dateTime"/>
								<xsd:element name="ToDate" type="xsd:dateTime" minOccurs="0"/>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="Extensions" type="extensionTransportMode"/>
					<xsd:element name="RegistrationNumber" type="xsd:string" minOccurs="0"/>
					<xsd:element name="OperationalNumber" type="xsd:string"/>
					<xsd:element name="PrivateCode" type="PrivateCodeStructure" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Hier altijd type="VehicleNumber" gebruiken</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="OperatorRef" type="VersionOfObjectRefStructure" minOccurs="0"/>
					<xsd:element name="VehicleTypeRef" type="VersionOfObjectRefStructure"/>
				</xsd:sequence>
				<xsd:attribute name="responsibilitySetRef" type="ObjectIdType" form="unqualified"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="vehicleType">
		<xsd:complexContent>
			<xsd:extension base="DataManagedObjectStructure">
				<xsd:sequence>
					<xsd:element name="Extensions" type="extensionTypeOfProductCategory" minOccurs="0"/>
					<xsd:element name="BrandingRef" type="VersionOfObjectRefStructure" minOccurs="0"/>
					<xsd:element name="Name" type="MultilingualString"/>
					<xsd:element name="ShortName" type="MultilingualString" minOccurs="0"/>
					<xsd:element name="Description" type="MultilingualString"/>
					<xsd:element name="PrivateCode" type="PrivateCodeStructure" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Hier altijd type="VoertuigTypeCode" gebruiken</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="EuroClass" type="xsd:normalizedString" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Zie Wikipedia voor de mogelijke waarden</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ReversingDirection" type="xsd:boolean" default="true" minOccurs="0"/>
					<xsd:element name="SelfPropelled" type="xsd:boolean" default="true" minOccurs="0"/>
					<xsd:element name="PropulsionType" type="PropulsionTypeEnumeration" minOccurs="0"/>
					<xsd:choice>
						<xsd:element name="FuelType" type="FuelTypeEnumeration">
							<xsd:annotation>
								<xsd:documentation>Het type brandstof dat voor het voertuig in gebruik is voor het voertuig soort.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="TypeOfFuel" type="FuelTypeEnumeration">
							<xsd:annotation>
								<xsd:documentation>DEPRECATED NAME: Use FuelType instead. Will be removed: 2025-01-09.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:choice>
					<xsd:element name="MaximumRange" type="DistanceType" minOccurs="0"/>
					<xsd:element name="TransportMode" type="TransportmodeEnum"/>
					<xsd:element name="capacities" type="passengerCapacities_RelStructure" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Capaciteit per tariefklasse</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="LowFloor" type="xsd:boolean"/>
					<xsd:element name="HasLiftOrRamp" type="xsd:boolean"/>
					<xsd:element name="HasHoist" type="xsd:boolean" minOccurs="0"/>
					<xsd:element name="BoardingHeight" type="LengthType" minOccurs="0"/>
					<xsd:element name="GapToPlatform" type="LengthType" minOccurs="0"/>
					<xsd:element name="Length" type="LengthType"/>
					<xsd:element name="Width" type="LengthType" minOccurs="0"/>
					<xsd:element name="Height" type="LengthType" minOccurs="0"/>
					<xsd:element name="Weight" type="WeightType" minOccurs="0"/>
					<xsd:element name="FirstAxleHeight" type="LengthType" minOccurs="0"/>
					<xsd:element name="facilities" type="facilities"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="passengerCapacities_RelStructure">
		<xsd:annotation>
		<xsd:documentation>Type for a list of PASSENGER CAPACITY REQUIREMENTs.</xsd:documentation>
		</xsd:annotation>
		<xsd:choice maxOccurs="unbounded">
			<xsd:element name="PassengerCapacityRef" type="VersionOfObjectRefStructure"/>
			<xsd:element ref="PassengerCapacity" maxOccurs="1"/>
		</xsd:choice>
	</xsd:complexType>
	<xsd:element name="PassengerCapacity">
		<xsd:annotation>
		<xsd:documentation>Capacity for a VEHICLE TYPE and Class.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
		<xsd:complexContent>
			<xsd:restriction base="PassengerCapacityStructure">
			<xsd:sequence>
				<xsd:group ref="PassengerCapacityGroup"/>
			</xsd:sequence>
			<xsd:attribute name="id" type="PassengerCapacityIdType" use="required" form="unqualified">
				<xsd:annotation>
				<xsd:documentation>Identifier of PASSENGER CAPACITY.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			</xsd:restriction>
		</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="PassengerCapacityStructure">
		<xsd:annotation>
			<xsd:documentation>Capacity for a VEHICLE TYPE and Class.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="DataManagedObjectStructure">
				<xsd:sequence>
					<xsd:group ref="PassengerCapacityGroup"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:simpleType name="PassengerCapacityIdType">
		<xsd:annotation>
			<xsd:documentation>Type for identifier of a PASSENGER CAPACITY.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="ObjectIdType"/>
	</xsd:simpleType>
	<xsd:group name="PassengerCapacityGroup">
		<xsd:annotation>
		<xsd:documentation>Elements for a PASSENGER CAPACITY REQUIREMENT. relevant for passenger systems.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
		<xsd:element name="FareClass" type="FareClassEnumeration" default="any" minOccurs="0">
			<xsd:annotation>
			<xsd:documentation>Edit fare class for which capacity is specifyed. Default is any, i.e. capacity is for all classes.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:group ref="CapacityGroup"/>
		</xsd:sequence>
	</xsd:group>
	<xsd:group name="CapacityGroup">
		<xsd:annotation>
		<xsd:documentation>Elements for a PASSENGER CAPACITY REQUIREMENT. relevant for passenger systems.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
		<xsd:element name="TotalCapacity" type="xsd:nonNegativeInteger" minOccurs="0">
			<xsd:annotation>
			<xsd:documentation>The total capacity of vehicles of the type. For a requirement this is the minimum needed.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="SeatingCapacity" type="xsd:nonNegativeInteger" minOccurs="0">
			<xsd:annotation>
			<xsd:documentation>The seating capacity of vehicles of the type. For a requirement this is the minimum needed.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="StandingCapacity" type="xsd:nonNegativeInteger" minOccurs="0">
			<xsd:annotation>
			<xsd:documentation>The standing capacity of vehicles of the type. For a requirement this is the minimum needed.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="SpecialPlaceCapacity" type="xsd:nonNegativeInteger" minOccurs="0">
			<xsd:annotation>
			<xsd:documentation>The number of special places on vehicles of the type, e.g. seats for the disabled. For a requirement this is the minimum needed.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="PushchairCapacity" type="xsd:nonNegativeInteger" minOccurs="0">
			<xsd:annotation>
			<xsd:documentation>The number of push chair places on vehicles of the type. For a requirement this is the minimum needed.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		<xsd:element name="WheelchairPlaceCapacity" type="xsd:nonNegativeInteger" minOccurs="0">
			<xsd:annotation>
			<xsd:documentation>The number of wheelchairs places on vehicles of the type. For a requirement this is the minimum needed.</xsd:documentation>
			</xsd:annotation>
		</xsd:element>
		</xsd:sequence>
	</xsd:group>
	<xsd:complexType name="facilities">
		<xsd:sequence>
			<xsd:element name="ServiceFacilitySet" type="serviceFacilitySet" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="serviceFacilitySet">
		<xsd:complexContent>
			<xsd:extension base="DataManagedObjectStructure">
				<xsd:sequence>
					<!--
					<xsd:element name="otherFacilities" minOccurs="0">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="TypeOfEquipmentRef" type="VersionOfObjectRefStructure"/>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
-->
					<xsd:element name="MobilityFacilityList" minOccurs="0">
						<xsd:simpleType>
							<xsd:list itemType="MobilityFacilityEnumeration"/>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="PassengerCommsFacilityList" minOccurs="0">
						<xsd:simpleType>
							<xsd:list itemType="PassengerCommsFacilityEnumeration"/>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="SanitaryFacilityList" minOccurs="0">
						<xsd:simpleType>
							<xsd:list itemType="SanitaryFacilityEnumeration"/>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="TicketingServiceFacilityList" minOccurs="0">
						<xsd:simpleType>
							<xsd:list itemType="TicketingServiceFacilityEnumeration"/>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="VehicleAccessFacilityList">
						<xsd:simpleType>
							<xsd:list itemType="AccessFacilityEnumeration"/>
							<!--	<xsd:list itemType="VehicleAccessFacilityEnumeration"/>	-->
						</xsd:simpleType>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="flexibleStopPlace">
		<xsd:complexContent>
			<xsd:extension base="DataManagedObjectStructure">
				<xsd:sequence>
					<xsd:element name="Name" type="MultilingualString" minOccurs="0"/>
					<xsd:element name="ShortName" type="MultilingualString"/>
					<xsd:element name="Description" type="MultilingualString" minOccurs="0"/>
					<!--
					<xsd:element name="PurposeOfGroupingRef" type="VersionOfObjectRefStructure">
						<xsd:annotation>
							<xsd:documentation>BISON enumeratie.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="PrivateCode" type="PrivateCodeStructure" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Hier altijd type="InternalName" gebruiken.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
-->
					<xsd:element name="TransportMode" type="TransportmodeEnum"/>
					<xsd:element name="PublicCode" type="MultilingualString" minOccurs="0"/>
					<xsd:element name="areas" minOccurs="0">
						<xsd:complexType>
							<xsd:choice minOccurs="0">
								<xsd:element name="FlexibleArea" type="flexibleArea"/>
								<xsd:element name="HailAndRideArea" type="hailAndRideArea"/>
							</xsd:choice>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="flexibleArea">
		<xsd:complexContent>
			<xsd:extension base="EmbeddedObjectStructure">
				<xsd:sequence>
					<xsd:element name="Name" type="MultilingualString" minOccurs="0"/>
					<xsd:element name="ShortName" type="MultilingualString"/>
					<xsd:element name="Description" type="MultilingualString" minOccurs="0"/>
					<!--
					<xsd:element name="PrivateCode" type="PrivateCodeStructure" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Hier altijd type="InternalName" gebruiken.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="types" minOccurs="0">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="TypeOfZoneRef" type="VersionOfObjectRefStructure">
									<xsd:annotation>
										<xsd:documentation>BISON enumeratie.</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
-->
					<xsd:element name="members" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Alleen gebruiken als het gebied inliggende haltes heeft. De referenties verwijzen naar de RoutePoints die de haltes representeren.</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="RoutePointRef" type="VersionOfObjectRefStructure" maxOccurs="unbounded"/>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element ref="gml:Polygon" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Alleen gebruiken als het gebied GEEN inliggende haltes heeft.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="BoardingUse" type="xsd:boolean"/>
					<xsd:element name="AlightingUse" type="xsd:boolean"/>
					<!--
					<xsd:element name="destinations" minOccurs="0">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="DestinationDisplayRef" type="VersionOfObjectRefStructure"/>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
-->
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="hailAndRideArea">
		<xsd:complexContent>
			<xsd:extension base="EmbeddedObjectStructure">
				<xsd:sequence>
					<xsd:element name="Name" type="MultilingualString" minOccurs="0"/>
					<xsd:element name="ShortName" type="MultilingualString"/>
					<xsd:element name="Description" type="MultilingualString" minOccurs="0"/>
					<!--
					<xsd:element name="PrivateCode" type="PrivateCodeStructure" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Hier altijd type="InternalName" gebruiken.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="types" minOccurs="0">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="TypeOfZoneRef" type="VersionOfObjectRefStructure">
									<xsd:annotation>
										<xsd:documentation>BISON enumeratie.</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
-->
					<xsd:element name="BoardingUse" type="xsd:boolean"/>
					<xsd:element name="AlightingUse" type="xsd:boolean"/>
					<!--
					<xsd:element name="destinations" minOccurs="0">
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="DestinationDisplayRef" type="VersionOfObjectRefStructure"/>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
-->
					<xsd:element name="StartPointRef" type="VersionOfObjectRefStructure">
						<xsd:annotation>
							<xsd:documentation>De referentie verwijst naar het RoutePoint aan het begin van de HailAndRide sectie.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="EndPointRef" type="VersionOfObjectRefStructure">
						<xsd:annotation>
							<xsd:documentation>De referentie verwijst naar het RoutePoint aan het einde van de HailAndRide sectie.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
</xsd:schema>
